<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="good_reading.BookReview" table="BookReview" lazy="false">
		<id name="_rid" column="_rid" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="_bid" column="_bid" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="_review" column="_review" type="string" length="255" not-null="false" lazy="false"/>
		<property name="_costumerName" column="_costumerName" type="string" length="255" not-null="false" lazy="false"/>
		<property name="_approved" column="_approved" type="boolean" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
