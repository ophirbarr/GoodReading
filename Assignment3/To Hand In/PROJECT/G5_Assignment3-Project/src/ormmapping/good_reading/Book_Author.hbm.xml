<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="good_reading.Book_Author" table="`Book-Author`" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="_bid" column="_bid" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="_author" column="_author" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
