<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="personnel.Worker" table="Worker" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="FirstName" column="FirstName" type="string" length="255" not-null="false" lazy="false"/>
		<property name="LastName" column="LastName" type="string" length="255" not-null="false" lazy="false"/>
		<property name="Email" column="Email" type="string" length="255" not-null="false" lazy="false"/>
		<property name="Role" column="Role" type="string" length="255" not-null="false" lazy="false"/>
		<property name="Department" column="Department" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
